      "\n",
      "=============================================\n",
      "ðŸ” Memproses: Dataset dengan Fitur Lengkap dengan CatBoost\n",
      "=============================================\n",
      "ðŸ“Š Akurasi Test  : 0.9920\n",
      "ðŸ“Š Presisi Test  : 0.9903\n",
      "ðŸ“Š Recall Test   : 0.9894\n",
      "ðŸ“Š F1-Score Test : 0.9899\n",
      "âœ… Akurasi Train : 1.0000\n",
      "ðŸ“‰ Celah Akurasi : 0.0080 â†’ Status: Baik\n",
      "\n",
      "=============================================\n",
      "ðŸ” Memproses: Dataset dengan Fitur Pilihan dengan CatBoost\n",
      "=============================================\n",
      "ðŸ“Š Akurasi Test  : 0.9917\n",
      "ðŸ“Š Presisi Test  : 0.9904\n",
      "ðŸ“Š Recall Test   : 0.9886\n",
      "ðŸ“Š F1-Score Test : 0.9895\n",
      "âœ… Akurasi Train : 1.0000\n",
      "ðŸ“‰ Celah Akurasi : 0.0083 â†’ Status: Baik\n",
      "\n",
      "\n",
      "âœ… Semua proses dengan CatBoost telah selesai.\n"
     ]
    }
   ],
   "source": [
    "# Siapkan list kosong untuk menyimpan hasil\n",
    "results = []\n",
    "\n",
    "# Loop untuk kedua dataset\n",
    "for name, df in [('Dataset dengan Fitur Lengkap', df1), ('Dataset dengan Fitur Pilihan', df2)]:\n",
    "    print(f\"\\n=============================================\")\n",
    "    print(f\"ðŸ” Memproses: {name} dengan CatBoost\")\n",
    "    print(f\"=============================================\")\n",
    "\n",
    "    # 1. Tentukan X dan y (TARGETNYA ADALAH 'cluster')\n",
    "    X = df.drop('cluster', axis=1)\n",
    "    y = df['cluster']\n",
    "\n",
    "    # 2. Train-test split\n",
    "    X_train, X_test, y_train, y_test = train_test_split(\n",
    "        X, y, test_size=0.2, stratify=y, random_state=42\n",
    "    )\n",
    "\n",
    "    # 3. Scaling DILAKUKAN SETELAH SPLIT\n",
    "    scaler = StandardScaler()\n",
    "    X_train_scaled = scaler.fit_transform(X_train)\n",
    "    X_test_scaled = scaler.transform(X_test)\n",
    "\n",
    "    # 4. Model CatBoost\n",
    "    # Kita gunakan parameter yang kuat untuk CatBoost\n",
    "    model = CatBoostClassifier(\n",
    "        iterations=1000,\n",
    "        learning_rate=0.1,\n",
    "        depth=8,\n",
    "        subsample=0.6,\n",
    "        l2_leaf_reg=3,\n",
    "        verbose=0, # Tidak menampilkan output training\n",
    "        random_state=42\n",
    "    )\n",
    "\n",
    "    # 5. Fit model\n",
    "    model.fit(X_train_scaled, y_train)\n",
    "\n",
    "    # 6. Prediksi\n",
    "    y_test_pred = model.predict(X_test_scaled)\n",
    "    y_train_pred = model.predict(X_train_scaled)\n",
    "\n",
    "    # 7. Evaluasi\n",
    "    acc = accuracy_score(y_test, y_test_pred)\n",
    "    prec = precision_score(y_test, y_test_pred)\n",
    "    rec = recall_score(y_test, y_test_pred)\n",
    "    f1 = f1_score(y_test, y_test_pred)\n",
    "    train_acc = accuracy_score(y_train, y_train_pred)\n",
    "    acc_gap = train_acc - acc\n",
    "    overfit_status = \"Cenderung Overfitting\" if acc_gap > 0.05 else \"Baik\"\n",
    "\n",
    "    # Cetak hasil untuk dataset ini\n",
    "    print(f\"ðŸ“Š Akurasi Test  : {acc:.4f}\")\n",
    "    print(f\"ðŸ“Š Presisi Test  : {prec:.4f}\")\n",
    "    print(f\"ðŸ“Š Recall Test   : {rec:.4f}\")\n",
    "    print(f\"ðŸ“Š F1-Score Test : {f1:.4f}\")\n",
    "    print(f\"âœ… Akurasi Train : {train_acc:.4f}\")\n",
    "    print(f\"ðŸ“‰ Celah Akurasi : {acc_gap:.4f} â†’ Status: {overfit_status}\")\n",
    "\n",
    "    # Simpan hasil ke dalam list\n",
    "    results.append({\n",
    "        'Dataset': name,\n",
    "        'Accuracy': acc,\n",
    "        'Precision': prec,\n",
    "        'Recall': rec,\n",
    "        'F1 Score': f1,\n",
    "        'Train Accuracy': train_acc,\n",
    "        'Gap': acc_gap,\n",
    "        'Status': overfit_status\n",
    "    })\n",
    "\n",
    "print(\"\\n\\nâœ… Semua proses dengan CatBoost telah selesai.\")"
   ]
  },
