    "df1 = pd.read_csv('/kaggle/input/fp-mlcatboostdataset/cluster1_standardscaler (1).csv')\n",
    "df2 = pd.read_csv('/kaggle/input/fp-mlcatboostdataset/cluster2_standardscaler_feature (1).csv')\n",
